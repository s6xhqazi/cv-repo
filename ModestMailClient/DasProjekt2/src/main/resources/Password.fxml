<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="200.0" prefWidth="483.0" spacing="25.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PasswordScene">
   <children>
      <Label text="Bitte geben Sie das Passwort ein:">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <PasswordField fx:id="pass" maxHeight="50.0" maxWidth="242.0" prefHeight="30.0" prefWidth="242.0" />
      <Button fx:id="login" onAction="#checkit" mnemonicParsing="false" text="Log In" />
   </children>
</VBox>
